<hr />
{{#with service}}
  <div class='row'>
    <div class='col-12 mt-4 mb-3 text-center'>
      <div class='alert text-bg-success' role='alert'>
        <h2>{{title}}</h2>
        <p>{{description}}</p>
        <div>
          {{#each buttons}}
            <a
              class='btn btn-primary btn-sm ms-2'
              href='{{link}}'
              role='button'
            >{{text}}</a>
          {{/each}}
        </div>
      </div>
    </div>
  </div>
{{/with}}